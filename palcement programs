#include<stdio.h>
void main() {
    int max2,max1,n,i,a[20];
    printf("Enter n:");
    scanf("%d",&n);
    printf("Enter %d elements:\n",n);
    for(i=0;i<n;i++)
        scanf("%d",&a[i]);
    for(i=0;i<n;i++)
        printf("%d\t",a[i]);
    max1=max2=a[0];
    for(i=0;i<n;i++) {
        if(a[i]>max1) {
            max2=max1;
            max1=a[i];
        }
        else if(a[i]>max2 && a[i]<max1)
            max2=a[i];
    }
    printf("\nlargest:%d\tsencond:%d",max2, max2);
}

#include<stdio.h>
#include<stdlib.h>
int main(){
   int a[50],i,j,k,n;
   printf("Enter n:");
   scanf("%d",&n);
   printf("Enter elements\n");
   for(i=0;i<n;i++){
      scanf("%d",&a[i]);
   }
   printf("Entered elements\n");
   for(i=0;i<n;i++){
      printf("%d ",a[i]);
   }
   for(i=0;i<n;i++){
      for(j=i+1;j<n;j++){
         if(a[i]==a[j]){
            for(k=j;k<n;k++)
               a[k]=a[k+1];
            n--;
         }
      }
   }
   printf("\nAfter deleting\n");
   for(i=0;i<n;i++){
      printf("%d\t",a[i]);
   }
}


#include<stdio.h>
#include<stdlib.h>
int main(){
   int a[50],i,j,k,n,count;
   printf("Enter n:");
   scanf("%d",&n);
   printf("Enter elements\n");
   for(i=0;i<n;i++){
      scanf("%d",&a[i]);
   }
   printf("Entered elements\n");
   for(i=0;i<n;i++){
      printf("%d ",a[i]);
   }
   for(i=0;i<n;i++){
      for(j=i+1;j<n;j++){
         if(a[i]==a[j]){
            count+=1;
            break;
         }
      }
   }
   printf("\ntotal:%d \n",count);
}

#include <stdio.h>
int main()
{
    int a[50],n,i,j,t[50],ele,count=0;
    printf("enter n:");
        scanf("%d",&n);
    printf("Enter eles:\n");
    for(i=0;i<n;i++) {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++) {
        ele=a[i];
        if(ele==t[count]) {
            continue;
        } 
        for(j=i+1;j<n;j++) {
            if (a[j]==ele) {
                t[count++] = ele;
                break;
            }
        }
    }
    printf("Count:%d\n", count);
    printf("Repeat eles are:");
    for(i=0;i<count;i++) {
        printf("%d ",t[i]);
    }
    return 0;
}


#include <stdio.h>
int main()
{
    int a[50],n,i,j,ele,count=0,t[50];
    printf("enter n:");
        scanf("%d",&n);
    printf("Enter eles:\n");
    for(i=1;i<n;i++) {
        scanf("%d",&a[i]);
    }
    for(i=1;i<n;i++) {
        ele=a[i];
        if(ele==t[count]) {
            continue;
        } 
        for(j=i+1;j<n;j++) {
            if (a[j]==ele) {
                t[count++] = ele;
                break;
            }
        }
    }
    printf("Count:%d\n", count);
    printf("Repeat eles are:");
    for(i=0;i<count;i++) {
        printf("%d ",t[i]);
    }
    return 0;
}

#include <stdio.h>
int main()
{
    int a[50],key,n,i,j,t[50];
    printf("enter n:");
        scanf("%d",&n);
    printf("Enter eles:\n");
    for(i=0;i<n;i++) {
        scanf("%d",&a[i]);
    }
    printf("Enter key:");
        scanf("%d",&key);
    for(i=0;i<n;i++) {
        for(j=i+1;j<n;j++) {
            if((a[i]+a[j]) == key)
                printf("%d & %d\n",a[i],a[j]);
        }
    }
    
}

