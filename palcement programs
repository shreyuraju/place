// to display the largest difference from a given set of n no.
#include<stdio.h>
void main() {
    int bg,sm,n,i,a[20],dif=0;
    printf("Enter n:");
    scanf("%d",&n);
    printf("Enter %d elements:\n",n);
    for(i=0;i<n;i++)
        scanf("%d",&a[i]);
    bg=sm=a[0];
    for(i=1;i<n;i++) {
        if(a[i]>bg)
            bg=a[i];
        if(a[i]<sm)
            sm=a[i];
        if(dif<(bg-sm))
            dif=(bg-sm);
        printf("\ndiffernce %d",(bg-sm));
        printf(" & its bettween %d & %d",bg,sm);
    }
    printf("\nlargest difference is %d",dif);
}

//program to rotate an array 10 times
#include<stdio.h>
void main() {
    int m,n,i,j,k,a[20],t=0,t1=0;
    printf("Enter n:");
    scanf("%d",&n);
    printf("Enter %d elements:\n",n);
    for(i=0;i<n;i++)
        scanf("%d",&a[i]);
    printf("Enter no.of rotation:\n",n);
        scanf("%d",&m);
    printf("Array elements:\n");
    for(k=0;k<n;k++)
        printf("%d  ",a[k]);
    printf("\n");
    printf("Array elements:\n");
    for(i=0;i<m;i++) {
        t1=a[0];
        for(j=0;j<n;j++) {
            t=a[j];
            a[j]=a[j+1];
            a[j+1]=t;
        }
        a[n-1]=t1;
        for(k=0;k<n;k++)
            printf("%d  ",a[k]);
        printf("\n");
    }
}
