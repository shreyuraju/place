#include <stdio.h>
int main(void) {
	int x,n,i,sum1=0,sum=0;
	printf("Enter size n:");
	    scanf("%d",&n);
	printf("Enter random 1- %d no.",n);
	for(i=1;i<=n;i++) {
	    scanf("%d",&x);
	    sum1+=x;
	}
    sum=(n+1)*(n+2)/2;
	printf("missing no.is %d",(sum-sum1));

}

#include <stdio.h>
#include<stdlib.h>
int main(void) {
	int t;
	printf("Enter the temperature:");
	    scanf("%d",&t);
	if(t<=0) {
	    printf("Freezing");
	    exit(0);
	}
	switch(t/10) {
	    case 0:printf("Very cold");
	            break;
	   case 1:printf("Cold");
	            break;
	   case 2:printf("Normal");
	            break;
	   case 3:printf("HOT");
	            break;
	   case 4:printf("Very Hot");
	            break;
	   default:printf("Extreme Hot");
	}
}

#include <stdio.h>
int main() {
    int n,i,j;
    printf("Enter n value:");
        scanf("%d",&n);
    for(i=0;i<n;i++) {
        for(j=0;j<n+1;j++)
            printf("*\t");
        printf("\n");
    }
    return 0;
}


#include <stdio.h>

void main() {
    int i,j,n;
    printf("Enter n:");
        scanf("%d",&n);
    for(i=1;i<=n;i++) {
        for(j=1;j<=i;j++)
            printf("*");
        printf("\n");
    }
}

#include <stdio.h>
void main() {
    int i,j,n=5;
    for(i=1;i<=n;i++) {
        for(j=1;j<=n;j++) {
            if((i==1) || (i==n) || (j==1) || (j==n))
                printf("*\t");
            else 
                printf("\t");
        }
        printf("\n");
    }
}


#include <stdio.h>
void main() {
    int i,j,n;
    printf("Enter n:");
        scanf("%d",&n);
    for(i=1;i<=n;i++) {
        for(j=0;j<=n;j++) {
            if((i==1) || (i==n) || (j==0) || (j==n))
                printf("*\t");
            else 
                printf("\t");
        }
        printf("\n");
    }
}

#include <stdio.h>
void main() {
    int i,n=6;
    while(n>0)  {
        for(i=1;i<=n;i++)
            printf("*");
        printf("\n");
        n--;
    }
}

#include <stdio.h>
int main() {
   int i, space, rows, k = 0;
   printf("Enter the number of rows: ");
   scanf("%d", &rows);
   for (i = 1; i <= rows; ++i, k = 0) {
      for (space = 1; space <= rows - i; ++space) {
         printf("  ");
      }
      while (k != 2 * i - 1) {
         printf("* ");
         ++k;
      }
      printf("\n");
   }
   return 0;
}

#include <stdio.h>
void main() {
    int i,j,n,k=0;
    printf("Enter n:");
        scanf("%d",&n);
    for(i=1;i<=n;++i,k=0) {
        for(j=1;j<=n-i;++j)
            printf(" ");
        while(k!=(2*i-1)) {
            printf("*");
            ++k;
        }
        printf("\n");
    }
}
07/12/21 {
#include <stdio.h>
void main() {
    int i,j,n,s=0,k;
    printf("Enter n:")
        scanf("%d",&n);
    n=n*2-1;
    for(i=n;i!=0;i=i-2,s++) {
        for(j=0;j<s;j++)
            printf(" ");
        for(k=0;k<=i-1;k++)
            printf("*");
        printf("\n");
    }
}

#include <stdio.h>
#include<math.h>
void main() {
	int m,n,max,min;
    scanf("%d",&m);
    scanf("%d",&n);
    max=((m+n)+abs(m-n))/2;
    min=((m+n)-abs(m-n))/2;
    printf("largest of %d & %d no.:%d",m,n,max);
    printf("\nsmallest of %d & %d no.:%d",m,n,min);
}


}

cn lab

print("Enter 1st sentence")
s1=input()
print('enter 2nd sentence')
s2=input()
l1=s1.split(' ')
l2=s2.split(' ')
print(l1)
print(l2)
com=[]
ncom=[]
for i in l1:
  if i in l2:
    if i not in com:
      com.append(i)
  else:
    ncom.append(i)
for j in l2:
  if j in l1:
    if j not in com:
      com.append(j)
  else:
    ncom.append(j)
print(com)
print(ncom)
